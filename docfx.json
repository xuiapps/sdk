{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": ".",
          "files": [
            "Xui/Core/Core/Xui.Core.csproj",
            "Xui/Core/Core.DI/Xui.Core.DI.csproj",
            "Xui/Core/Fonts/Xui.Core.Fonts.csproj",
            "Xui/Middleware/Emulator/Emulator.csproj",
            "Xui/Runtime/Android/Android.csproj",
            "Xui/Runtime/Browser/Browser.csproj",
            "Xui/Runtime/IOS/IOS.csproj",
            "Xui/Runtime/MacOS/MacOS.csproj",
            "Xui/Runtime/Software/Xui.Runtime.Software.csproj",
            "Xui/Runtime/Windows/Windows.csproj"
          ]
        }
      ],
      "output": "www/api",
      "namespaceLayout": "nested",
      "allowCompilationErrors": true
    }
  ],
  "build": {
    "content": [
      {
        "files": ["toc.yml", "docs/**/*.{md,yml}"],
        "src": "www"
      },
      {
        "files": ["api/**/*.{md,yml}"],
        "src": "www"
      }
    ],
    "resource": [
      {
        "files": ["**"],
        "src": "www/index"
      },
      {
        "files": ["docs/**/*.svg"],
        "src": "www"
      }
    ],
    "output": "_site",
    "template": ["default", "modern", "www/templates/xui"],
    "globalMetadata": {
      "_appName": "Xui Apps",
      "_appTitle": "Xui Apps",
      "_enableSearch": true,
      "_disableContribution": true,
      "pdf": false
    }
  }
}
